#include <stdio.h>

/**
 * main - Entry point
 * Description: Prints the alphabet in lowercase followed by new line
 * Except letter 'q' and 'e'
 * Return: Always 0 (Success)
 */
int main(void)
{
char c;

for (c = 'a'; c <= 'z'; c++)

if (c == 'e' || c == 'q')
continue;
putchar(c);

putchar('\n');
return (0);
}
